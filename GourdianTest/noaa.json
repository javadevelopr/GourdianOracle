{
    "dataset":{
        "name":"noaa",
        "sources": [
            {
                "name":"global_summary_of_day", 
                "columns": [
                    "STATION",
                    "DATE",
                    "LATITUDE",
                    "LONGITUDE",
                    "ELEVATION",
                    "NAME",
                    "TEMP",
                    "TEMP_ATTRIBUTES",
                    "DEWP",
                    "DEWP_ATTRIBUTES",
                    "SLP",
                    "SLP_ATTRIBUTES",
                    "STP",
                    "STP_ATTRIBUTES",
                    "VISIB",
                    "VISIB_ATTRIBUTES",
                    "WDSP",
                    "WDSP_ATTRIBUTES",
                    "MXSPD",
                    "GUST",
                    "MAX",
                    "MAX_ATTRIBUTES",
                    "MIN",
                    "MIN_ATTRIBUTES",
                    "PRCP",
                    "PRCP_ATTRIBUTES",
                    "SNDP",
                    "FRSHTT"
                ],
                "layouts":[
                    {
                        "name":"approxlat_approxlng_yyyy_mm",
			"transform": "",
                        "keys":[
                            "Latitude",
                            "Longitude",
                            "Date Local"
                        ],
			"sort_order" : "desc",
                        "max_chunk_length": "250000"
                    },
                    {
                        "name":"yyyy_mm_approxlat_approxlng",
			"transform": "",
                        "keys":[
                            "Date Local",
                            "Latitude",
                            "Longitude"
                        ],
			"sort_order": "desc",
                        "max_chunk_length": "250000"
                    }
                ]
                    
                
            }


        ]

    }

}
